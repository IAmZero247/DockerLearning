

---
version: "3"
services:
  application:
    image: ruby4eyes/spring-boot-mysql-app
    networks:
      - springboot-mysqldb-net
    ports:
      - "8080:8080"
    depends_on:
      - mysqldb
  mysqldb:
    image: mysql:5.7
    networks:
      - springboot-mysqldb-net
    environment:
      - MYSQL_ROOT_PASSWORD=root
      - MYSQL_DATABASE=sbms
networks:
  - springboot-mysqldb-net:
...